{
    "compilerOptions": {
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "lib": [
            "es6"
        ],
        "inlineSourceMap": true,
        "rootDir": ".",
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules"
    ]
}
